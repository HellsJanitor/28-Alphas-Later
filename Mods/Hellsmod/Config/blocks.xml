<configs>
	<!-- search find all *** for changes -->
	
	<!-- *** double planted crops growth time *** -->
	<set xpath="//block[@name='cropsGrowingMaster']//property[@name='PlantGrowing.GrowthRate']/@value">126</set>
	
	<!-- *** adds tool slots to workbench *** -->
	<set xpath="//block[@name='workbench']/property[2]//property[1]/@value">tools,output</set>
	
	<!-- *** adds fuel to cement mixer *** -->
	<set xpath="//block[@name='cementMixer']/property[2]/property[@name='Modules']/@value">output,fuel</set>
	<append xpath="//block[@name='cementMixer']">
	<property name="Fuels" value="ammoGasCan,100"/>
	</append>
	
	<append xpath="//block[@name='campfire']">
	<property name="HasModifiers" value="true"/>
	</append>
	
	<!-- *** increased drop rate for honey from vanilla stumps *** -->
	<set xpath="//block[@name='treeStump']//drop[2]/@prob">.75</set>
	
	<!-- *** adds honeybee harvest from trees with WastelandTreasures book *** -->
	<insertafter xpath="//block[@name='treeMaster']//drop[@event='Harvest'][@name='foodHoney']">
	<drop event="Harvest" name="honeyBee" prob=".05" count="1" tag="WasteTreasuresHoneyHarvest"/>
	</insertafter>
	
	<!-- *** player crafted tablesaw *** -->
	<insertbefore xpath="//block[@name='tableSaw']">
	<block name="playerTableSaw">
		<property name="CustomIcon" value="tableSaw"/>
		<property name="Class" value="Workstation"/>
		<property class="Workstation">
			<property name="Modules" value="output"/>
			<property name="CraftingAreaRecipes" value="playerTableSaw"/>
		</property>
		<property name="UnlockedBy" value="craftingWorkstations"/>
		<property name="CreativeMode" value="Player"/>
		<property name="DescriptionKey" value="tableSawDesc"/>
		<property name="DisplayType" value="blockMulti"/>
		<property name="WorkstationIcon" value="ui_game_symbol_table_saw"/>
		<property name="WorkstationJournalTip" value="tablesawTip"/>
		<property name="Stacknumber" value="1"/>
		<property name="Material" value="Miron_scrap"/>
		<property name="Shape" value="ModelEntity"/>
		<property name="Model" value="Entities/Crafting/table_sawPrefab"/>
		<property name="ModelOffset" value="0,.5,0"/>
		<property name="MultiBlockDim" value="1,1,1"/>
		<property name="ImposterDontBlock" value="true"/>
		<property name="WaterFlow" value="permitted"/>
		<property name="Place" value="TowardsPlacerInverted"/>
		<property name="OnlySimpleRotations" value="true"/>
		<property name="IsTerrainDecoration" value="true"/>
		<property name="StabilitySupport" value="false"/>
		<property name="Weight" value="0"/>
		<property name="EconomicValue" value="2000"/>
		<property name="TraderStageTemplate" value="midTier1"/>
		<property name="MaxDamage" value="800"/>
		<property name="HeatMapStrength" value="5"/>
		<property name="HeatMapTime" value="5000"/>
		<property name="HeatMapFrequency" value="1000"/>
		<property name="OpenSound" value="open_workbench"/>
		<property name="CloseSound" value="close_workbench"/>
		<property name="TakeDelay" value="15"/>
		<property class="RepairItems">
			<property name="resourceForgedIron" value="10"/>
			<property name="resourceSpring" value="2"/>
			<property name="resourceMechanicalParts" value="4"/>
		</property>
		<drop event="Harvest" name="resourceScrapIron" count="300" tag="allHarvest"/>
		<drop event="Harvest" name="terrStone" count="0" tool_category="Disassemble"/>
		<drop event="Harvest" name="resourceForgedIron" count="8" tag="salvageHarvest"/>
		<drop event="Harvest" name="resourceSpring" count="1" tag="salvageHarvest"/>
		<drop event="Harvest" name="resourceMechanicalParts" count="3" tag="salvageHarvest"/>
		<drop event="Destroy" count="0"/>
		<drop event="Fall" name="scrapMetalPile" count="1" prob="0.75" stick_chance="1"/>
		<property name="SortOrder1" value="B281"/>
		<property name="SortOrder2" value="0300"/>
		<property name="Group" value="Basics,Building,advBuilding"/>
		<property name="Tags" value="workstationSkill,twitch_workstation"/>
		<property name="FilterTags" value="MC_playerBlocks,SC_decor"/>
	</block>		
	</insertbefore>
	
